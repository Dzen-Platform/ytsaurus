{
    "log_tailer" = {
        "log_files" = [
            {
                "path" = "LOG";
                "tables" = [
                    {
                        "path" = "//sys/clickhouse/logs/log1";
                    };
                    {
                        "path" = "//sys/clickhouse/logs/log2";
                        "require_trace_id" = %true;
                    };
                ];
            }
        ];
        "log_rotation" = {
            "enable" = %true;
            "rotation_period" = 100;
            "rotation_delay" = 50;
            "log_segment_count" = 100000;
        };
        "log_writer_liveness_checker" = {
            "enable" = %true;
            "liveness_check_period" = 50;
        };
        "read_frequency" = 50;
    };
    "logging" = {
        "writers" = {
            "debug" = {
                "file_name" = "LOG";
                "type" = "file";
            };
            "info" = {
                "file_name" = "LOG";
                "type" = "file";
            };
        };
        "rules" = [
            {
                "min_level" = "debug";
                "writers" = [
                    "debug";
                ];
            };
            {
                "min_level" = "info";
                "writers" = [
                    "info";
                ];
            };

        ];
        "flush_period" = 1000;
        "watch_period" = 5000;
    };
}
