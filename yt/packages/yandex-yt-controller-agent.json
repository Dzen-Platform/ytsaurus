{
    "meta": {
        "name": "yandex-yt-controller-agent",
        "maintainer": "YT Team <yt@yandex-team.ru>",
        "description": "YT. This package provides server-side software",
        "version": "{revision}.{branch}",
        "pre-depends": [],
        "depends": ["libc6 (>= 2.15)"],
        "conflicts": ["yandex-yt"],
        "homepage": "https://wiki.yandex-team.ru/yt/"
    },
    "build": {
        "targets": [
            "yt/server/programs/controller_agent"
        ]
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "yt/server/programs/controller_agent/ytserver-controller-agent"
            },
            "destination": {
                "path": "/usr/bin/ytserver-controller-agent"
            }
        }
    ]
}
