{
    cluster=freud;
    pool="yt-integration-tests";
    cache_path="//home/yt-integration-tests";
    cache_ttl_hours=24;
    coordinate_upload=%true;

    output_path="//home/yt-integration-tests/output";
    output_ttl_hours=168;

    upload_binaries=[
        "yt/python/yt/wrapper/ytrecipe/yt-python-yt-wrapper-ytrecipe";
        "yt/python/yt/wrapper/ytrecipe/py2_yt-python-yt-wrapper-ytrecipe";
        
        "yt/yt/server/all/ytserver-all";
        "yt/yt/server/all/stripped/ytserver-all-stripped";
        "yt/yt/tools/yt_sudo_fixup/yt-sudo-fixup";
        
        "infra/nanny/logrotate/logrotate";
        "yt/python/yt/environment/bin/yt_env_watcher_make/yt_env_watcher";

        "yt/yt/server/clickhouse_trampoline/clickhouse-trampoline";
        "yt/yt/server/clickhouse_server/bin/ytserver-clickhouse";
        "yt/yt/server/log_tailer/bin/ytserver-log-tailer";
        "yt/yt/experiments/ytserver_dummy/ytserver-dummy";

        "yt/python/yt/wrapper/bin/yt_make/yt";
        "yt/python/yt/wrapper/tests/yt_python/yt-python";
        "yt/python/yt/wrapper/tests/yt_python3/yt-python3";
        "yt/python/yt/wrapper/tests/yt_ipython/yt-ipython";
        "yt/python/yt/wrapper/tests/files/cpp_bin/cpp_bin";
    ];

    upload_workdir=[
        "layers";
        "rootfs";
        "cpp_bin_core_crash";
    ];

    resource_limits={
        cpu_limit=8;
        memory_limit=10000000000;
        tmpfs_size=10000000000;
    };

    job_timeout_seconds=2400;
}
