{
    "meta": {
        "name": "yandex-yt-fennel",
        "maintainer": "Kolesnichenko Ignat <ignat@yandex-team.ru>",
        "description": "Fennel reliably push rows from an YT scheduler event log table to the Logbroker",
        "version": "{revision}"
    },
    "build": {
        "targets": [
            "yt/python/yt/fennel/bin/fennel"
        ]
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "yt/python/yt/fennel/bin/fennel/fennel"
            },
            "destination": {
                "path": "/usr/bin/fennel"
            }
        }
    ]
}
