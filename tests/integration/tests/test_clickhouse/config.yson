{
    "logging" = {
        "rules" = [
            {
                "min_level" = "debug";
                "writers" = [
                    "debug";
                ];
            };
            {
                "min_level" = "info";
                "writers" = [
                    "info";
                ];
            };
            {
                "min_level" = "error";
                "writers" = [
                    "stderr";
                ];
            };
        ];
        "writers" = {
            "info" = {
                "file_name" = "./clickhouse-$YT_JOB_INDEX.log";
                "type" = "file";
            };
            "debug" = {
                "file_name" = "./clickhouse-$YT_JOB_INDEX.debug.log";
                "type" = "file";
            };
            "stderr" = {
                "type" = "stderr";
            };
        };
    };
    "addresses" = [
        [
            "interconnect";
            "localhost";
        ];
        [
            "default";
            "localhost";
        ];
    ];
    "address_resolver" = {
        "localhost_fqdn" = "localhost";
    };
    "rpc_port" = 32236;
    "monitoring_port" = 32238;
    "validate_operation_access" = %false;
    "user" = "root";
    "engine" = {
        "settings" = {
            "max_temporary_non_const_columns" = 1234;
            "max_threads" = 1;
            "max_distributed_connections" = 1;
            "enable_optimize_predicate_expression" = 0;
            "join_use_nulls" = 1;
        };
    };
    "discovery" = {
        "directory" = "//sys/clickhouse/cliques";
        "update_period" = 500;
        "transaction_timeout" = 1000;
    };
}
