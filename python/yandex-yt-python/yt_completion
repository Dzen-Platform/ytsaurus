#!/bin/bash

_yt_argcomplete() {
    local IFS=''
    COMPREPLY=( $(
        IFS="$IFS" COMP_LINE="$COMP_LINE" COMP_POINT="$COMP_POINT" _ARGCOMPLETE_COMP_WORDBREAKS="$COMP_WORDBREAKS" _ARGCOMPLETE=1 "$1" 8>&1 9>&2 1>/dev/null 2>/dev/null) 
    )
    if [[ $? != 0 ]]; then
        unset COMPREPLY
    fi
}

complete -o nospace -F _yt_argcomplete "yt2"
complete -o nospace -F _yt_argcomplete "yt"
